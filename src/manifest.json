{
  "name": "Tab Counter",
  "author": "tizee",
  "manifest_version": 2,
  "version": "1.0.1",
  "description": "Displays the number of opened tabs in the current window",
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "tizee@wtf.org"
    }
  }
}
